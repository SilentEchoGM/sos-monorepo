{
  "$schema": "https://turborepo.org/schema.json",

  "pipeline": {
    "install": {
      "dependsOn": ["^install"]
    },
    "build": {
      "dependsOn": ["^build"]
    },
    "dist": {
      "dependsOn": [
        "sos-emulator-svelte#build",
        "sos-emulator-electron#build",
        "^build"
      ]
    },
    "dev": {
      "cache": false
    }
  }
}
